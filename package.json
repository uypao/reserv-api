{
  "name": "resrev-api",
  "version": "1.0.0",
  "description": "Reservation mobile app for individuals providing services",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "debug": " babel-node-debug ./src/index.js --presets es2015",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    },
    "globals": {
      "Promise": true
    }
  },
  "author": "Paolo Uy",
  "license": "MIT",
  "dependencies": {
    "babel-node-debug": "^2.0.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "mongoose": "^4.11.4",
    "morgan": "^1.8.0",
    "node-inspector": "^1.1.1",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  },
  "keywords": [
    "reservation",
    "appointment",
    "calendar"
  ]
}
